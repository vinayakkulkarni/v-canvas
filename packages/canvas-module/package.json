{
  "name": "@vinayakkulkarni/canvas-module",
  "version": "0.0.2",
  "description": "DSCVR Canvas module for your awesome Nuxt app(s)",
  "main": "dist/module.js",
  "module": "dist/module.mjs",
  "types": "dist/module.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "bun postinstall",
    "postinstall": "nuxi prepare",
    "nuxt:dev": "nuxi dev",
    "build": "tsup src/module.ts --format cjs,esm --dts --tsconfig ./tsconfig.json",
    "dev": "tsup src/module.ts --format cjs,esm --dts --watch --tsconfig ./tsconfig.json",
    "lint": "bun run lint:eslint; bun run lint:prettier",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint . --fix",
    "lint:prettier": "prettier --check \"{,!(node_modules|dist)/**/}*.{js,ts}\"",
    "lint:prettier:fix": "prettier --write \"{,!(node_modules|dist)/**/}*.{js,ts}\"",
    "test": "bun test",
    "release": "release-it"
  },
  "peerDependencies": {
    "nuxt": "^3.12.4",
    "vite": "^5.4.0"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.4",
    "@vinayakkulkarni/canvas-client": "0.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@nuxt/module-builder": "^0.5.5",
    "@types/node": "^22.2.0",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "eslint": "^9.9.0",
    "nuxt": "^3.12.4",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "release-it": {
    "git": false,
    "plugins": {
      "@release-it/bumper": {
        "out": {
          "file": "package.json",
          "path": "dependencies.@vinayakkulkarni/canvas-client"
        }
      }
    }
  },
  "keywords": [
    "Nuxt.js",
    "Canvas",
    "DSCVR Canvas",
    "DSCVR Canvas Store",
    "Nuxt Canvas",
    "Nuxt DSCVR Canvas Module",
    "Nuxt Canvas Module"
  ],
  "author": {
    "name": "Vinayak Kulkarni",
    "email": "inbox.vinayak@gmail.com",
    "url": "https://vinayakkulkarni.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinayakkulkarni/v-canvas.git"
  },
  "private": false,
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/vinayakkulkarni/v-canvas/issues"
  },
  "homepage": "https://github.com/vinayakkulkarni/v-canvas#readme"
}
