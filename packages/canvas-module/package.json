{
  "name": "@vinayakkulkarni/canvas-module",
  "version": "0.0.9",
  "description": "DSCVR Canvas module for your awesome Nuxt app(s)",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/module.d.cts",
        "default": "./dist/module.cjs"
      },
      "import": {
        "types": "./dist/module.d.mts",
        "default": "./dist/module.mjs"
      },
      "types": "./dist/module.d.ts",
      "default": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist",
    "*.d.ts",
    "*.cjs",
    "*.mjs"
  ],
  "scripts": {
    "prebuild": "bun postinstall",
    "postinstall": "nuxi prepare",
    "build:stub": "nuxt-build-module --stub",
    "build:module": "nuxt-build-module",
    "build": "rimraf dist && bun build:module",
    "lint": "bun run lint:eslint; bun run lint:prettier",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint . --fix",
    "lint:prettier": "prettier --check \"{,!(node_modules|dist|.nuxt)/**/}*.{js,ts}\"",
    "lint:prettier:fix": "prettier --write \"{,!(node_modules|dist|.nuxt)/**/}*.{js,ts}\"",
    "test": "bun test",
    "dev": "nuxi dev",
    "release": "release-it"
  },
  "peerDependencies": {
    "nuxt": "^3.12.4",
    "vite": "^5.4.0"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.4",
    "@vinayakkulkarni/canvas-client": "0.0.9",
    "defu": "^6.1.4"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.8.3",
    "nuxt": "^3.12.4"
  },
  "keywords": [
    "Nuxt.js",
    "Canvas",
    "DSCVR Canvas",
    "DSCVR Canvas Store",
    "Nuxt Canvas",
    "Nuxt DSCVR Canvas Module",
    "Nuxt Canvas Module"
  ],
  "author": {
    "name": "Vinayak Kulkarni",
    "email": "inbox.vinayak@gmail.com",
    "url": "https://vinayakkulkarni.dev"
  },
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinayakkulkarni/v-canvas.git"
  },
  "private": false,
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/vinayakkulkarni/v-canvas/issues"
  },
  "homepage": "https://github.com/vinayakkulkarni/v-canvas#readme"
}
