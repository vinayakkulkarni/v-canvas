{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "types": [
      "@types/node"
    ],
    "baseUrl": ".",
    "paths": {
      "#imports": [
        "./node_modules/nuxt/app.d.ts"
      ],
      "#app": [
        "./node_modules/nuxt/dist/app"
      ]
    }
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
